const int sensor_pin = 8; 

void setup() {
  Serial.begin(9600); 
  pinMode(sensor_pin, INPUT);
} 

void loop() {
  int estado = digitalRead(sensor_pin); 

  if (estado == LOW) {
    Serial.println("Objeto detectado"); 
  } else {
    Serial.println("No hay objeto detectado");      
  }

  delay(200); 
}
